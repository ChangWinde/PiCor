[flake8]
max-line-length = 100
exclude = 
    .git,
    __pycache__,
    .venv,
    venv,
    build,
    dist,
    *.egg-info
ignore = 
    E203,
    E501,
    W503,
    F401,
    F403,
    E722,
    E731,
    W291,
    W293,
    E302,
    E303,
    E305,
    E306,
    E261,
    E262,
    E265,
    E266,
    E401,
    E402,
    E711,
    E712,
    E713,
    E714,
    E721,
    E741,
    E742,
    E743,
    E901,
    E902,
    F841 